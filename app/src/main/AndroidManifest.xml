<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.rajul.cas">

    <uses-permission android:name="android.permission.ACCESS_NETWORK_STATE" />
    <uses-permission android:name="android.permission.INTERNET" />

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity
            android:name=".HomeTeacher"
            android:label="Attendance Portal"
            android:theme="@style/AppTheme.NoActionBar" />
        <activity
            android:name=".Login"
            android:theme="@style/AppTheme.NoActionBar" />
        <activity
            android:name=".AskLogin"
            android:theme="@style/AppTheme.NoActionBar" />
        <activity
            android:name=".asksignup"
            android:theme="@style/AppTheme.NoActionBar">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity
            android:name=".signup"
            android:theme="@style/AppTheme.NoActionBar" />
        <activity
            android:name=".StudentDashboard"
            android:label="Student Attendance Portal"
            android:theme="@style/AppTheme.NoActionBar" />
        <activity
            android:name=".Teacherprofileentry"
            android:label="Teacher Profile Update"
            android:theme="@style/AppTheme.NoActionBar" />
        <activity
            android:name=".AttendanceUpload"
            android:label="Attendance Upload"
            android:theme="@style/AppTheme.NoActionBar" />
        <activity
            android:name=".viewAttendance"
            android:label="View Attendance"
            android:theme="@style/AppTheme.NoActionBar" />
        <activity
            android:name=".StudentProfileEntry"
            android:label="Student Profile Update"
            android:theme="@style/AppTheme.NoActionBar" />
        <activity
            android:name=".StudentProfile"
            android:label="Student Profile"
            android:theme="@style/AppTheme.NoActionBar" />
        <activity
            android:name=".EditProfile"
            android:label= "Edit Profile"
            android:theme="@style/AppTheme.NoActionBar">

        </activity>
    </application>

</manifest>